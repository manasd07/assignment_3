{
  "name": "e-commerce",
  "version": "1.0.0",
  "description": "E-commerce based backend application",
  "main": "dist/src/index.js",
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "nodemon ./src/index.ts --exec ts-node babel-node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Manas Dwivedi",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.12",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "husky": "^4.3.8",
    "lint-staged": "^11.0.0",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.7",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "typeorm": "^0.2.34",
    "typescript": "^4.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "prettier": "^2.3.1"
  }
}
